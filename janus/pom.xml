	<distributionManagement>
		<repository>
			<id>releases.devmas.utbm.fr</id>
			<name>SeT Release Repository</name>
			<url>file:///var/www/janus/maven/</url>
		</repository>
		<snapshotRepository>
			<id>snapshots.devmas.utbm.fr</id>
			<name>SeT Snapshots Repository</name>
			<url>file:///var/www/janus/maven/</url>
